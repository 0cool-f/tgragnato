<?xml version="1.0" encoding="UTF-8"?>
<urlset
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd"
    xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
    xmlns:image="http://www.google.com/schemas/sitemap-image/1.1">
  <url>
    <loc>{{ site.url }}/index.html</loc>
    <changefreq>monthly</changefreq>
    {% for image in page.images %}
    <image:image>
      <image:loc>{{ site.images | prepend: site.url }}/{{ image.loc }}</image:loc>
      {% if image.cap %}<image:caption>{{ image.cap }}</image:caption>{% else %}<image:caption>{{ site.data.base.name }} {{ site.data.base.surname }}</image:caption>{% endif %}
      {% if image.tit %}<image:title>{{ image.tit }}</image:title>{% else %}<image:title>{{ site.data.base.name }} {{ site.data.base.surname }}</image:title>{% endif %}
      <image:license>https://raw.githubusercontent.com/{{ site.data.contacts.github }}/master/LICENSE</image:license>
    </image:image>
    {% endfor %}
  </url>
</urlset>
